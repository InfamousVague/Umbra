{
  "expo": {
    "name": "Umbra",
    "slug": "Umbra",
    "version": "1.7.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "umbra",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1241"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "chat.umbra.app",
      "appleTeamId": "F6ZAL7ANAD",
      "associatedDomains": [
        "applinks:umbra.chat"
      ],
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Umbra needs camera access for video calls and effects preview."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1A1241"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "chat.umbra.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "umbra.chat",
              "pathPrefix": "/invite/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "buildReactNativeFromSource": true,
            "deploymentTarget": "16.0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "959a0873-4bf3-48f6-8baa-3fd074e9d87b"
      }
    }
  }
}
