{
  "name": "umbra",
  "main": "expo-router/entry",
  "version": "1.2.0",
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "patch": "./scripts/patch-wisp.sh",
    "presync": "node scripts/postinstall.js",
    "start": "node scripts/postinstall.js && expo start",
    "android": "expo run:android --device",
    "ios": "expo run:ios --device",
    "web": "node scripts/postinstall.js && expo start --web",
    "build:web": "node scripts/postinstall.js && npx expo export --platform web",
    "build:wasm": "./scripts/build-wasm.sh",
    "build:mobile": "./scripts/build-mobile.sh",
    "build:mobile:ios": "./scripts/build-mobile.sh ios-sim",
    "build:mobile:ios:device": "./scripts/build-mobile.sh ios",
    "build:mobile:android": "./scripts/build-mobile.sh android",
    "prebuild": "npx expo prebuild --clean",
    "run:ios": "./scripts/build-mobile.sh ios-sim && npx expo prebuild --clean && npx expo run:ios --device",
    "run:android": "./scripts/build-mobile.sh android && npx expo prebuild --clean && npx expo run:android --device",
    "build:desktop": "./scripts/build-desktop.sh",
    "build:mac": "./scripts/build-mac.sh",
    "build:win": "./scripts/build-win.sh",
    "build:linux": "./scripts/build-linux.sh",
    "build:desktop:dev": "./scripts/build-desktop.sh --dev",
    "build:desktop:debug": "./scripts/build-desktop.sh --debug",
    "build:desktop:clean": "./scripts/build-desktop.sh --clean",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "npx playwright test --config=__tests__/e2e/playwright.config.ts",
    "deploy": "./scripts/deploy.sh",
    "deploy:frontend": "./scripts/deploy.sh frontend",
    "deploy:relay": "./scripts/deploy.sh relay"
  },
  "dependencies": {
    "@coexist/wisp-core": "^0.1.0",
    "@coexist/wisp-react-native": "^0.1.0",
    "@expo/vector-icons": "^15.0.3",
    "@mediapipe/tasks-vision": "^0.10.32",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/native": "^7.1.8",
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-process": "^2.0.0",
    "@tauri-apps/plugin-updater": "^2.0.0",
    "@umbra/plugin-runtime": "file:packages/umbra-plugin-runtime",
    "@umbra/plugin-sdk": "file:packages/umbra-plugin-sdk",
    "@umbra/service": "file:packages/umbra-service",
    "@umbra/wasm": "file:packages/umbra-wasm",
    "expo-umbra-core": "file:modules/expo-umbra-core",
    "expo": "~54.0.33",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.13",
    "expo-font": "~14.0.11",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "jszip": "^3.10.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "sql.js": "^1.10.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tauri-apps/cli": "^2.10.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.17",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "react-native-svg": "15.12.1"
  },
  "resolutions": {
    "react-native-svg": "15.12.1"
  },
  "private": true
}
